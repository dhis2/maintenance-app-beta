{
    "name": "project-doom",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "yarn localize && react-scripts build",
        "eject": "react-scripts eject",
        "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
        "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/",
        "test": "react-scripts test",
        "lint:style": "d2-style js check --all",
        "prestart": "yarn localize"
    },
    "dependencies": {
        "@dhis2/app-runtime": "^2.0.4",
        "@dhis2/d2-i18n": "^1.0.6",
        "@dhis2/d2-i18n-extract": "^1.0.8",
        "@dhis2/d2-i18n-generate": "^1.1.1",
        "@dhis2/prop-types": "^1.4.0",
        "@dhis2/ui-core": "^3.12.0",
        "@dhis2/ui-widgets": "2.0.1",
        "d2-manifest": "^1.0.0",
        "history": "^4.10.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-redux": "^7.1.3",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.2.0",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "reselect": "^4.0.0"
    },
    "devDependencies": {
        "@dhis2/cli-style": "4.2.0"
    }
}
